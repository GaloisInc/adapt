{
    "netflow" : {
        "description" : "Analyze netflow nodes i.e. network sockets for possible anomalous behavior",
        "instance_set" : "IDS=g.V().has(ETYPE,CONNECT).out(E_A_N_O).out(E_A_N_I).dedup().id().toList().toArray()",
        "feature_set" : {
            "write_bytes":
                "g.V(IDS).local(__.in(E_A_N_I).in(E_A_N_O).has(ETYPE,WRITE).values(SIZE).sum())",
            "read_bytes":
                "g.V(IDS).local(__.out(N_A_E_O).out(N_A_E_I).has(ETYPE,READ).values(SIZE).sum())",
            "num_writes":
                "g.V(IDS).local(__.in(E_A_N_I).in(E_A_N_O).has(ETYPE,WRITE).count())",
            "num_reads":
                "g.V(IDS).local(__.out(N_A_E_O).out(N_A_E_I).has(ETYPE,READ).count())",
            "num_open":
                "g.V(IDS).local(__.out(N_A_E_O).out(N_A_E_I).has(ETYPE,OPEN).count())",
            "num_close":
                "g.V(IDS).local(__.in(E_A_N_I).in(E_A_N_O).has(ETYPE,CLOSE).count())"
        }
    },

    "netflow_connect_process_exec" : {
        "description" : "Analyze netflow nodes that have connection with file execution (potential remote execution command)",
        "instance_set" : "IDS=g.V().has(ETYPE,CONNECT).out(E_A_N_O).out(E_A_N_I).as('netflow').both(E_A_N_I,N_A_E_O).both(E_A_N_O,N_A_E_I).out(E_G_B_S_O).out(E_G_B_S_I).in(E_G_B_S_I).in(E_G_B_S_O).has(ETYPE,EXECUTE).select('netflow').dedup().id().toList().toArray()",
        "feature_set" : {
            "write_bytes":
                "g.V(IDS).local(__.in(E_A_N_I).in(E_A_N_O).has(ETYPE,WRITE).values(SIZE).sum())",
            "read_bytes":
                "g.V(IDS).local(__.out(N_A_E_O).out(N_A_E_I).has(ETYPE,READ).values(SIZE).sum())",
            "num_writes":
                "g.V(IDS).local(__.in(E_A_N_I).in(E_A_N_O).has(ETYPE,WRITE).count())",
            "num_reads":
                "g.V(IDS).local(__.out(N_A_E_O).out(N_A_E_I).has(ETYPE,READ).count())",
            "num_open":
                "g.V(IDS).local(__.out(N_A_E_O).out(N_A_E_I).has(ETYPE,OPEN).count())",
            "num_close":
                "g.V(IDS).local(__.in(E_A_N_I).in(E_A_N_O).has(ETYPE,CLOSE).count())",
            "duration":{
                "first":
                    "g.V(IDS).local(__.both(E_A_N_I,N_A_E_O).both(E_A_N_O,N_A_E_I).has(STYPE,EVENT).values(STIME).max())",
                "second":
                    "g.V(IDS).local(__.in(E_A_N_I).in(E_A_N_O).has(ETYPE,CONNECT).values(STIME).min())",
                "operator":
                    "RELUTime"
            }
        }
    },

